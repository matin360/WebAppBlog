@model IEnumerable<BlogWeb.WebUI.Areas.Admin.Models.EntityReportModel>
@{
	ViewBag.Title = "Dashboard";
	Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<div class="row">
	@if (Model.Any()) {
		foreach (var item in Model)
		{
			Html.RenderPartial("_EntityCountPartial", item);
		}
	}
</div>
